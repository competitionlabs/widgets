{
  "name": "@competitionlabs/leaderboard",
  "version": "3.0.5",
  "description": "Leaderboard Widgets",
  "scripts": {
    "dev": "cross-env --env.LANG=en --env.production=dev --env.THEME=default-theme webpack-dev-server --config ./webpack/webpack.config.js --progress",
    "dev:dark-theme": "cross-env --env.LANG=en --env.production=dev --env.THEME=dark-theme webpack-dev-server --config ./webpack/webpack.config.js --progress",
    "dev:blue-theme": "cross-env --env.LANG=en --env.production=dev --env.THEME=blue-theme webpack-dev-server --config ./webpack/webpack.config.js --progress",
    "dev:green-theme": "cross-env --env.LANG=en --env.production=dev --env.THEME=green-theme webpack-dev-server --config ./webpack/webpack.config.js --progress",
    "dev:grey-theme": "cross-env --env.LANG=en --env.production=dev --env.THEME=grey-theme webpack-dev-server --config ./webpack/webpack.config.js --progress",
    "dev:red-theme": "cross-env --env.LANG=en --env.production=dev --env.THEME=red-theme webpack-dev-server --config ./webpack/webpack.config.js --progress",
    "dev:orange-theme": "cross-env --env.LANG=en --env.production=dev --env.THEME=orange-theme webpack-dev-server --config ./webpack/webpack.config.js --progress",
    "dev:black-theme": "cross-env --env.LANG=en --env.production=dev --env.THEME=black-theme webpack-dev-server --config ./webpack/webpack.config.js --progress",
    "build": "rimraf build && cross-env --env.LANG=en --env.production=prod --env.THEME=default-theme webpack --config ./webpack/webpack.config.js --progress",
    "build:dark-theme": "rimraf build && cross-env --env.LANG=en --env.production=prod --env.THEME=dark-theme webpack --config ./webpack/webpack.config.js --progress",
    "build:blue-theme": "rimraf build && cross-env --env.LANG=en --env.production=prod --env.THEME=blue-theme webpack --config ./webpack/webpack.config.js --progress",
    "build:green-theme": "rimraf build && cross-env --env.LANG=en --env.production=prod --env.THEME=green-theme webpack --config ./webpack/webpack.config.js --progress",
    "build:grey-theme": "rimraf build && cross-env --env.LANG=en --env.production=prod --env.THEME=grey-theme webpack --config ./webpack/webpack.config.js --progress",
    "build:red-theme": "rimraf build && cross-env --env.LANG=en --env.production=prod --env.THEME=red-theme webpack --config ./webpack/webpack.config.js --progress",
    "build:orange-theme": "rimraf build && cross-env --env.LANG=en --env.production=prod --env.THEME=orange-theme webpack --config ./webpack/webpack.config.js --progress",
    "build:black-theme": "rimraf build && cross-env --env.LANG=en --env.production=prod --env.THEME=black-theme webpack --config ./webpack/webpack.config.js --progress",
    "build:inline-css": "rimraf build && cross-env --env.LANG=en env.INLINE_CSS=true --env.THEME=default-theme --env.production=prod webpack --config ./webpack/webpack.config.js --progress",

    "lint": "eslint ./src",
    "example": "webpack-dev-server --open --port 9001 --contentBase ../ --open-page /examples/leaderboard_v3.html",
    "example:loader": "webpack-dev-server --open --port 9001 --contentBase ../ --open-page /examples/leaderboard_v3_loader.html",
    "example:customized": "webpack-dev-server --open --port 9001 --contentBase ../ --open-page /examples/leaderboard_v3_customized.html",
    "example:inline-css": "webpack-dev-server --open --port 9001 --contentBase ../ --open-page /examples/leaderboard_v3_inline_css.html",
    "example:npm": "webpack-dev-server --open --port 9001 --contentBase ../ --open-page /examples/leaderboard_v3_npm/dist/",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:coverage": "webpack-dev-server --open --port 9002 --contentBase ./coverage/lcov-report",
    "jsdoc": "rimraf docs && jsdoc -c jsdoc.json",
    "jsdoc:open": "webpack-dev-server --open --port 9003 --contentBase ./docs"
  },
  "author": "COMPETITION LABS LTD",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.1",
    "docdash": "^1.2.0",
    "eslint": "^7.2.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.0.0",
    "jest": "^26.3.0",
    "jsdoc": "^3.6.5",
    "nan": "^2.14.1",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.2",
    "style-loader": "^1.2.1",
    "svg-url-loader": "^6.0.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "css-vars-ponyfill": "^2.3.2",
    "identicon.js": "^2.3.3",
    "jssha": "^3.1.0",
    "moment": "2.24.0",
    "node-forge": "^0.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/competitionlabs/widgets/tree/master/leaderboard_v3"
  },
  "main": "build/javascript/leaderboard.v3.js",
  "style": "build/css/style.css"
}
